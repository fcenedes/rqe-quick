# -----------------------------
# Redis Connection Settings
# -----------------------------
# Redis host (e.g., "localhost" or "redis-11127.internal.c48004.eu-west9-mz.gcp.cloud.rlrcp.com")
REDIS_HOST=localhost

# Redis port (e.g., 6379 or 11127)
REDIS_PORT=6379

# Redis database number
REDIS_DB=0

# Redis username (leave empty for no auth, or "default")
REDIS_USERNAME=

# Redis password (leave empty for no auth)
REDIS_PASSWORD=

# -----------------------------
# Performance Settings
# -----------------------------
# NOTE: For single Redis instance, parallelism has minimal benefit since Redis
#       is single-threaded. The real performance gain comes from larger batch sizes.
#       Use parallelism mainly for Redis Cluster or multiple Redis instances.

# Number of concurrent worker threads (reduced for single instance)
PARALLEL_WORKERS=4

# Size of Redis connection pool (reduced for single instance)
CONNECTION_POOL_SIZE=4

# Pipeline batch size for seeding (INCREASED - this is the real win)
SEED_BATCH_SIZE=20000

# Cursor batch size for aggregation (INCREASED - reduces round trips)
AGGREGATE_BATCH_SIZE=20000

# -----------------------------
# Data Generation Settings
# -----------------------------
# Random seed for reproducible data generation
# Same seed = same data every time (useful for consistent benchmarks)
# Change seed to generate different random data
RANDOM_SEED=42

